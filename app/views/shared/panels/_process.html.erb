<div id="<%= service.id -%>" class="panel panel-<%= panel_color(service.current_event) %>" data-id="<%= service.id -%>">
  <div class="panel-heading portlet-handler">
    <div class="row row-table row-flush">
      <div class="col-xs-8">
        <p class="mb0">PROCESS</p>
        <h3 class="m0"><%= service.name -%></h3>
      </div>
      <div class="col-xs-4 text-center">
        <em class="fa fa-gear fa-2x"></em>
      </div>
    </div>
  </div>
  <div class="panel-body">
    <!-- Bar chart-->
    <div class="text-center">
      <div data-chart-range-min="0" data-stacked-bar-color="#6AB550,#4B7F38" data-zero-color="#CCCCCC" data-null-color="#FFFFFF" data-height="30" data-bar-width="6" data-bar-spacing="6" class="inlinesparkline inline"><%= service.events.order_by(:created_at.desc).limit(34).map{|e| "#{e[:memory].nil? ? 0 : e.memory[:kilobyte].to_f.round}:#{e[:memory].nil? ? 0 : e.cpu[:percent].to_f.round}"}.compact.join(',') -%></div>
    </div>
  </div>
  <div class="panel-footer"><i class="fa fa-star-o add-favorite"></i><span class="pull-right">See more &nbsp; <i class="fa fa-external-link"></i></span></div>
</div>
