<div id="<%= service.id -%>" class="panel panel-success" data-id="<%= service.id -%>">
  <div class="panel-heading portlet-handler">
    <div class="row row-table row-flush">
      <div class="col-xs-8">
        <p class="mb0">PROGRAM</p>
        <h3 class="m0"><%= service.name -%></h3>
      </div>
      <div class="col-xs-4 text-center">
        <em class="fa fa-cube fa-2x"></em>
      </div>
    </div>
  </div>
  <div class="panel-body">
    <!-- Bar chart-->
    <div class="text-center">
      <div data-bar-color="<%= bar_color(service.current_event) %>" data-height="30" data-bar-width="6" data-bar-spacing="6" class="inlinesparkline inline"><%= service.events.order_by(:created_at.desc).limit(34).map{|e| e[:program]['status'].to_i unless e[:program].nil?}.compact.join(',') -%></div>
    </div>
  </div>
  <div class="panel-footer"><i class="fa fa-star-o add-favorite"></i><span class="pull-right">See more &nbsp; <i class="fa fa-external-link"></i></span></div>
</div>
